files:
  "/etc/nginx/conf.d/proxy.conf":
    mode: "000755"
    owner: root
    group: root
    content: |
        client_max_body_size 500M;
        upstream nodejs {
            server 127.0.0.1:3000;
            keepalive 256;
        }
option_settings:
    aws:elasticbeanstalk:container:nodejs:
        NodeCommand: "npm start"